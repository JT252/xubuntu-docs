#: desktop-guide/C/command-line.xml:59(userinput)
#, no-wrap
msgid "rm [file(s)]"
msgstr "rm [ficheiro(s)]"

#: desktop-guide/C/command-line.xml:59(userinput)
#, no-wrap
msgid "rm -ir [dir(s)]"
