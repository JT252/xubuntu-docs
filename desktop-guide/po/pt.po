msgstr "Mostra o conteúdo de um ficheiro de texto"

#: desktop-guide/C/command-line.xml:55(userinput)
#, no-wrap
msgid "less [file]"
msgstr "less [ficheiro]"

#: desktop-guide/C/command-line.xml:56(userinput)
#, no-wrap
msgid "ls"
msgstr "ls"

#: desktop-guide/C/command-line.xml:56(seg)
msgid ""
"Lists files in a directory. Lists files in the current directory if none is "
"specified"
msgstr ""
"Lista os ficheiros em um diretório. Lista os ficheiros no diretório atual se "
"nenhum for especificado"

#: desktop-guide/C/command-line.xml:56(userinput)
#, no-wrap
msgid "ls [directory]"
msgstr "ls [diretório]"

#: desktop-guide/C/command-line.xml:57(userinput)
#, no-wrap
msgid "man"
msgstr "mv [nome_ficheiro] [novo_nome_ficheiro]"

#: desktop-guide/C/command-line.xml:57(seg)
msgid "Shows help about a command"
msgstr "Exibe ajuda sobre um comando"

#: desktop-guide/C/command-line.xml:57(userinput)
#, no-wrap
msgid "man [command]"
